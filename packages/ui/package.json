{
  "name": "@my/ui",
  "version": "0.0.1",
  "sideEffects": [
    "*.css"
  ],
  "private": true,
  "main": "src/index.ts",
  "source": "src/index.ts",
  "types": "src/index.ts",
  "module:jsx": "src",
  "files": [
    "types",
    "dist"
  ],
  "dependencies": {
    "@tamagui/core": "^1.132.23",
    "@tamagui/lucide-icons": "^1.132.23",
    "tamagui": "^1.132.23"
  },
  "scripts": {
    "build": "tamagui-build --skip-types",
    "watch": "tamagui-build --skip-types --watch",
    "lint": "eslint ./src",
    "lint:fix": "eslint ./src --fix",
    "generate-theme": "tamagui generate-themes ./src/themes/theme.ts ./src/themes/theme-generated.ts",
    "check:type": "tsc --outDir /tmp",
    "check:type:watch": "yarn check:type -w",
    "check-circular-deps": "../../scripts/check-circular-deps.sh ./src/index.tsx"
  },
  "devDependencies": {
    "@tamagui/build": "^1.132.23"
  }
}
