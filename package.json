{
  "name": "mymonjiapp",
  "main": "expo-router/entry",
  "version": "1.0.0",
  "packageManager": "yarn@4.5.0",
  "private": true,
  "workspaces": [
    "packages/*"
  ],
  "scripts": {
    "upgrade:tamagui": "yarn up '*tamagui*'@latest '@tamagui/*'@latest",
    "upgrade:tamagui:canary": "yarn up '*tamagui*'@canary '@tamagui/*'@canary",
    "check:tamagui": "tamagui check",
    "clean:watchman": "watchman watch-del-all && watchman shutdown-server",
    "start": "expo start -c",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "web": "expo start --web",
    "test": "jest --watchAll"
  },
  "jest": {
    "preset": "jest-expo"
  },
  "resolutions": {
    "metro": "^0.82.0",
    "metro-config": "^0.82.0",
    "metro-resolver": "^0.82.0"
  },
  "dependencies": {
    "@my/ui": "*",
    "@react-navigation/native": "^7.1.6",
    "@tamagui/config": "^1.138.6",
    "@tamagui/font-silkscreen": "^1.138.6",
    "@tamagui/lucide-icons": "^1.138.6",
    "@tamagui/toast": "^1.138.6",
    "babel-preset-expo": "~54.0.0",
    "burnt": "^0.12.2",
    "expo": "^54.0.0",
    "expo-build-properties": "~1.0.10",
    "expo-constants": "~18.0.11",
    "expo-dev-client": "~6.0.20",
    "expo-font": "~14.0.10",
    "expo-linear-gradient": "~15.0.8",
    "expo-linking": "~8.0.10",
    "expo-router": "~6.0.17",
    "expo-splash-screen": "~31.0.12",
    "expo-status-bar": "~3.0.9",
    "expo-system-ui": "~6.0.9",
    "expo-web-browser": "~15.0.10",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "react-native": "0.81.5",
    "react-native-reanimated": "~4.1.1",
    "react-native-safe-area-context": "~5.6.0",
    "react-native-screens": "~4.16.0",
    "react-native-svg": "15.12.1",
    "react-native-web": "^0.21.0",
    "react-native-worklets": "0.5.1"
  },
  "devDependencies": {
    "@babel/core": "^7.24.6",
    "@tamagui/babel-plugin": "^1.138.6",
    "@tamagui/cli": "^1.138.6",
    "@tamagui/metro-plugin": "^1.138.6",
    "@types/react": "~19.1.10",
    "typescript": "~5.9.2"
  },
  "expo": {
    "doctor": {
      "reactNativeDirectoryCheck": {
        "enabled": true,
        "exclude": [
          "@my/ui",
          "@tamagui/demos",
          "@tamagui/font-silkscreen",
          "@tamagui/lucide-icons"
        ],
        "listUnknownPackages": true
      }
    }
  }
}
